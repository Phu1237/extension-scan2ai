{
  "name": "scan2ai",
  "version": "1.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "build:dev": "vite build --mode development",
    "preview": "vite preview",
